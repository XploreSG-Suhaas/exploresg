# XploreSG Development Environment Variables
# Copy this file to .env and fill in your API keys

# Google Services
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Weather Service
WEATHER_API_KEY=your_weather_api_key_here

# AI Service  
GEMINI_API_KEY=your_gemini_api_key_here

# Development Settings
NODE_ENV=development
PYTHON_ENV=development
GO_ENV=development

# Database URLs (automatically configured by Docker Compose)
DATABASE_URL=postgresql://postgres:postgres123@postgres:5432/xploresg
MONGODB_URL=mongodb://mongo:mongo123@mongodb:27017/xploresg
REDIS_URL=redis://:redis123@redis:6379